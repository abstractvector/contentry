{
  "name": "contentry",
  "version": "0.0.1",
  "description": "Expose WordPress through GraphQL",
  "main": "index.js",
  "scripts": {
    "start": "node build/index.js",
    "clean": "rm -rf build && mkdir build",
    "build-server": "babel --presets es2015 -d ./build ./src -s",
    "build": "npm run clean && npm run build-server",
    "watch": "nodemon -w src/ --exec \"babel-node --presets es2015 src/index.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abstractvector/contentry.git"
  },
  "keywords": [
    "wordpress",
    "cms",
    "graphql",
    "api"
  ],
  "author": "Matt Knight <abstractvector@users.noreply.github.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/abstractvector/contentry/issues"
  },
  "homepage": "https://github.com/abstractvector/contentry#readme",
  "dependencies": {
    "apollo-server-koa": "^1.3.2",
    "config": "^1.29.1",
    "graphql": "^0.12.3",
    "graphql-tools": "^2.18.0",
    "js-yaml": "^3.10.0",
    "koa": "^2.4.1",
    "koa-bodyparser": "^4.2.0",
    "koa-router": "^7.3.0",
    "mysql2": "^1.5.1",
    "sequelize": "^4.31.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "nodemon": "^1.14.10"
  }
}
